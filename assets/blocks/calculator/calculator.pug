section#calc.calc 
  .container
    .calc-container 
      // -----------------
      // Calculator header
      // -----------------
      .calc-header 
        .calc-header__title 
          span Узнайте сколько будет стоить 
            span.title_marked ваш ремонт
      // ---------------
      // Calculator form
      // ---------------
      form.calc-form
        // Tabs
        .calc-form-tabs 
          h3.calc-form-tabs__title Выберите вид ремонта
          .calc-form-tab-wrap
            input.calc-form-tab__radio-btn(type="radio" name="renovation-type") 
            span.calc-form-tab__name Косметический
          .calc-form-tab-wrap
            input.calc-form-tab__radio-btn(type="radio" name="renovation-type") 
            span.calc-form-tab__name Дизайнерский
          .calc-form-tab-wrap
            input.calc-form-tab__radio-btn(type="radio" name="renovation-type") 
            span.calc-form-tab__name "Под ключ"
        // Square
        .calc-form-square 
          h3.calc-form-square__title Укажите общую площадь м2
          input.calc__input(type="number", name="" placeholder="0" min="1")
        // Renovation type
        .calc-building-type
          h3.calc-building-type__title Выберите тип недвижимости
          .calc-building-type-wrap 
            input.calc-building-type__radio-btn(type="radio", name="building-type")
            span.calc-building-type__name Первичная недвижимость
          .calc-building-type-wrap
            input.calc-building-type__radio-btn(type="radio", name="building-type")
            span.calc-building-type__name Вторичная недвижимость
        // ---------------
        // Calculator tabs
        // ---------------
        .calc-tabs 
          a#defaultOpen.calc-tablink.active.firstTab(onclick="openCalc(event, 'apartment')") Квартира
          a.calc-tablink(onclick="openCalc(event, 'room')") Комната
          a.calc-tablink(onclick="openCalc(event, 'kitchen')") Кухня
          a.calc-tablink(onclick="openCalc(event, 'bathroom')") Ванная
          a.calc-tablink(onclick="openCalc(event, 'building')") Дом
          a.calc-tablink.lastTab(onclick="openCalc(event, 'office')") Офис
        // -----------------------
        // Apartment
        // -----------------------
        #apartment.calc-tabcontent
          .calc-form
            .calc-wrap.animate__fadeIn.animate__animated
              // -------------
              // Walls options
              // -------------
              .calc-box.walls
                h4.calc-services__title Стены
                p.calc-add-service 
                  input.calc-add-service__checkbox(type="checkbox", name="add-services") 
                  span.calc-add-service__name Выравнивание стен (штукатурка)
                p.calc-add-service 
                  input.calc-add-service__checkbox(type="checkbox", name="add-services") 
                  span.calc-add-service__name Выравнивание стен (гипсокартон)
                p.calc-add-service 
                  input.calc-add-service__checkbox(type="checkbox", name="add-services") 
                  span.calc-add-service__name Поклейка обоев
              // -------------
              // Floor options
              // -------------
              .calc-box.floors
                h4.calc-services__title Полы
                p.calc-add-service
                  input.calc-add-service__checkbox(type="checkbox", name="add-services") 
                  span.calc-add-service__name Выравнивание пола
                p.calc-add-service
                  input.calc-add-service__checkbox(type="checkbox", name="add-services") 
                  span.calc-add-service__name Финишное покрытие пола (ламинат)
                p.calc-add-service
                  input.calc-add-service__checkbox(type="checkbox", name="add-services") 
                  span.calc-add-service__name Финишное покрытие пола (плитка)
              // ---------------
              // Ceiling options
              // ---------------   
              .calc-box.ceilings
                h4.calc-services__title Потолок
                p.calc-add-service 
                  input.calc-add-service__checkbox(type="checkbox", name="add-services") 
                  span.calc-add-service__name Выравнивание потолка (штукатурка)
                p.calc-add-service 
                  input.calc-add-service__checkbox(type="checkbox", name="add-services") 
                  span.calc-add-service__name Потолки (гипсокартон)
                p.calc-add-service 
                  input.calc-add-service__checkbox(type="checkbox", name="add-services") 
                  span.calc-add-service__name Натяжные потолки
              // ------------------
              // Additional options
              // ------------------
              .calc-box.additional
                h4.calc-services__title Дополнительно
                p.calc-add-service
                  input.calc-add-service__checkbox(type="checkbox", name="add-services") 
                  span.calc-add-service__name Замена электрики
                p.calc-add-service 
                  input.calc-add-service__checkbox(type="checkbox", name="add-services") 
                  span.calc-add-service__name Замена системы водоснабжения
                p.calc-add-service 
                  input.calc-add-service__checkbox(type="checkbox", name="add-services") 
                  span.calc-add-service__name Замена системы отопления
                p.calc-add-service
                  input.calc__input-num(type="number", name="" placeholder="0" min="0")
                  span.calc-add-service__name Замена или монтаж радиаторов отопления, шт
                p.calc-add-service
                  input.calc__input-num(type="number", name="" placeholder="0" min="0")
                  span.calc-add-service__name Замена окон, шт
                p.calc-add-service
                  input.calc-add-service__checkbox(type="checkbox", name="add-services") 
                  span.calc-add-service__name Установка двери
                p.calc-add-service 
                  input.calc-add-service__checkbox(type="checkbox", name="add-services") 
                  span.calc-add-service__name Звукоизоляция стен, пола, потолка
                p.calc-add-service 
                  input.calc-add-service__checkbox(type="checkbox", name="add-services") 
                  span.calc-add-service__name Материал нашей компании
              .calc-box.calc-img-wrap
                img.calc-img(src="assets/images/dest/apartment.jpg" alt="apartment"  rel="prefetch prerender") 
            .calc-btn-container
                button.btn.calc__btn(type="submit") Рассчитать
