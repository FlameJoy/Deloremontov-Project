section#calc.calc 
  .container
    .calc-container 
      // -----------------
      // Calculator header
      // -----------------
      .calc-header 
        .calc-header__title 
          span Узнайте сколько будет стоить 
            span.title_marked ваш ремонт
      // ---------------
      // Calculator form
      // ---------------
      form.calc-form(action="assets/php/calculator.php" method="post" name="calc-form")
        // Tabs
        .calc-form-tabs 
          h3.calc-form-tabs__title Выберите вид ремонта
          .calc-form-tab-wrap
            input.calc-form-tab__radio-btn(type="radio" name="renovationType" value="cosmetic") 
            span.calc-form-tab__name Косметический
          .calc-form-tab-wrap
            input.calc-form-tab__radio-btn(type="radio" name="renovationType" value="design") 
            span.calc-form-tab__name Дизайнерский
          .calc-form-tab-wrap
            input.calc-form-tab__radio-btn(type="radio" name="renovationType" value="onkey") 
            span.calc-form-tab__name "Под ключ"
        // Square
        .calc-form-square 
          h3.calc-form-square__title Укажите общую площадь м2
          input#square.calc__input(type="number" name="square" placeholder="0" min="1" value="" required)
        // Renovation type
        .calc-building-type
          h3.calc-building-type__title Выберите тип недвижимости
          .calc-building-type-wrap 
            input.calc-building-type__radio-btn(type="radio" name="buildingType" value="new")
            span.calc-building-type__name Первичная недвижимость
          .calc-building-type-wrap
            input.calc-building-type__radio-btn(type="radio" name="buildingType" value="old")
            span.calc-building-type__name Вторичная недвижимость
        // ---------------
        // Calculator tabs
        // ---------------
        .calc-tabs 
          a#defaultOpen.calc-tablink.active.firstTab(onclick="openCalc(event, 'apartment')") Квартира
          a.calc-tablink(onclick="openCalc(event, 'room')") Комната
          a.calc-tablink(onclick="openCalc(event, 'kitchen')") Кухня
          a.calc-tablink(onclick="openCalc(event, 'bathroom')") Ванная
          a.calc-tablink(onclick="openCalc(event, 'building')") Дом
          a.calc-tablink.lastTab(onclick="openCalc(event, 'office')") Офис
        // -----------------------
        // Apartment
        // -----------------------
        #apartment.calc-tabcontent
          .calc-form
            .calc-wrap.animate__fadeIn.animate__animated
              // -------------
              // Walls options
              // -------------
              .calc-box.walls
                h4.calc-services__title Стены
                p.calc-add-service 
                  input.calc-add-service__checkbox(type="checkbox" name="plaster" id="wallPlaster" value="Выравнивание стен (штукатурка)") 
                  span.calc-add-service__name Выравнивание стен (штукатурка)
                p.calc-add-service 
                  input.calc-add-service__checkbox(type="checkbox" name="drywall" id="wallDrywall" value="Выравнивание стен (гипсокартон)") 
                  span.calc-add-service__name Выравнивание стен (гипсокартон)
                p.calc-add-service 
                  input.calc-add-service__checkbox(type="checkbox" name="wallpaper" id="wallWallpaper" value="Поклейка обоев") 
                  span.calc-add-service__name Поклейка обоев
              // -------------
              // Floor options
              // -------------
              .calc-box.floors
                h4.calc-services__title Полы
                p.calc-add-service
                  input.calc-add-service__checkbox(type="checkbox" name="floorLeveling" id="floorLeveling" value="floorLeveling") 
                  span.calc-add-service__name Выравнивание пола
                p.calc-add-service
                  input.calc-add-service__checkbox(type="checkbox" name="floorLeveling" id="floorLaminate" value="floorLaminate") 
                  span.calc-add-service__name Финишное покрытие пола (ламинат)
                p.calc-add-service
                  input.calc-add-service__checkbox(type="checkbox" name="floorTile" id="floorTile" value="floorTile") 
                  span.calc-add-service__name Финишное покрытие пола (плитка)
              // ---------------
              // Ceiling options
              // ---------------   
              .calc-box.ceilings
                h4.calc-services__title Потолок
                p.calc-add-service 
                  input.calc-add-service__checkbox(type="checkbox" name="ceilingPlaster" id="ceilingPlaster" value="ceilingPlaster") 
                  span.calc-add-service__name Выравнивание потолка (штукатурка)
                p.calc-add-service 
                  input.calc-add-service__checkbox(type="checkbox" name="ceilingDrywall" id="ceilingDrywall" value="ceilingDrywall") 
                  span.calc-add-service__name Потолки (гипсокартон)
                p.calc-add-service 
                  input.calc-add-service__checkbox(type="checkbox" name="stretchCeiling" id="stretchCeiling" value="stretchCeiling") 
                  span.calc-add-service__name Натяжные потолки
              // ------------------
              // Additional options
              // ------------------
              .calc-box.additional
                h4.calc-services__title Дополнительно
                p.calc-add-service
                  input.calc-add-service__checkbox(type="checkbox" name="electrics" id="electrics" value="electrics") 
                  span.calc-add-service__name Замена электрики
                p.calc-add-service 
                  input.calc-add-service__checkbox(type="checkbox" name="waterSupplySystem" id="waterSupplySystem" value="waterSupplySystem") 
                  span.calc-add-service__name Замена системы водоснабжения
                p.calc-add-service 
                  input.calc-add-service__checkbox(type="checkbox" name="heatingSystem" id="heatingSystem" value="heatingSystem") 
                  span.calc-add-service__name Замена системы отопления
                p.calc-add-service
                  input.calc__input-num(type="number" name="replHeatingRadiators" placeholder="0" min="0" value="0")
                  span.calc-add-service__name Замена или монтаж радиаторов отопления, шт
                p.calc-add-service
                  input.calc__input-num(type="number" name="replWindows" placeholder="0" min="0" value="0")
                  span.calc-add-service__name Замена окон, шт
                p.calc-add-service
                  input.calc-add-service__checkbox(type="checkbox", name="add-services") 
                  span.calc-add-service__name Установка двери
                p.calc-add-service 
                  input.calc-add-service__checkbox(type="checkbox", name="add-services") 
                  span.calc-add-service__name Звукоизоляция стен, пола, потолка
                p.calc-add-service 
                  input.calc-add-service__checkbox(type="checkbox", name="add-services") 
                  span.calc-add-service__name Материал нашей компании
              .calc-box.calc-img-wrap
                img.calc-img(src="assets/images/dest/apartment.jpg" alt="apartment"  rel="prefetch prerender") 
              // Email input
              h4.calc-estimate Введите адрес электронной почты
                input.calc__input.calc-estimate__adress(type="email" name="email" placeholder="example@gmail.com")
            .calc-btn-container
                button.btn.calc__btn(type="submit") Рассчитать
